#!/bin/bash
set -euo pipefail

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
ROOT_DIR="$( cd "$DIR"/../.. && pwd )"

. "$ROOT_DIR/config.sh"

channel=${1:-release}

ssh $DEPLOY_HOST cat "$MANIFEST_DEPLOY_PATH/$channel/updates.json" | jq -r '.addons["juris-m@juris-m.github.io"].updates[-1].version'
